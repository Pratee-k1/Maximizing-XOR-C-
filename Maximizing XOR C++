#include <sstream>
#include <iostream>
#include <algorithm>
using namespace std;
/*
 * Complete the function below.
 */
int maxXor(int l, int r) {
    int m=0,i,j;
    for(i=l;i<=r-1;i++)
        { for(j=i+1;j<=r;j++)
          m=max(m,i^j);
        }
    return m;  
}

int main() {
    int res;
    int _l;
    cin >> _l;
    
    int _r;
    cin >> _r;
    
    res = maxXor(_l, _r);
    cout << res;
    
    return 0;
}
